{
  "name": "Chen",
  "description": "Chen is one of the original Bootstrap theme forked from Honoka. It uses Japanese webfonts.",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "japanese",
    "theme",
    "responsive",
    "front-end",
    "framework",
    "web",
    "touhou",
    "honoka",
    "webfont"
  ],
  "author": "tamaina",
  "homepage": "https://tamaina.github.io/Chen",
  "repository": {
    "type": "git",
    "url": "git://github.com/tamaina/Chen.git"
  },
  "license": "MIT",
  "moduleType": "globals",
  "main": [
    "dist/css/bootstrap.min.css"
  ],
  "ignore": [
    "/.*",
    "dist/assets",
    "dist/*.html",
    "src"
  ],
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7"
  },
  "exportsOverride": {
    "bootstrap-sass": {},
    "bootstrap": {
      "js": "dist/js/bootstrap.**js",
      "fonts": "dist/fonts/**/*"
    },
    "jquery": {}
  }
}
